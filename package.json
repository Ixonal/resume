{
  "name": "resume",
  "description": "Benjamin McGregor's Resume App",
  "version": "0.1.0",
  "repository": {
    "type": "GIT",
    "url": "https://ixonal.github.io/resume"
  },
  "license": "MIT",
  "scripts": {
    "start": "au run --watch",
    "build": "npm run build:dev",
    "build:dev": "au build --env dev && npm run build:sw",
    "build:stage": "au build --env stage && npm run build:sw",
    "build:prod": "au build --env prod && npm run build:sw",
    "build:sw": "sw-precache --config=swconfig.json",
    "build:deploy": "npm run build:deploy:icons && npm run build:deploy:manifest && npm run build:deploy:sw",
    "build:deploy:icons": "cpx \"images/**/*.*\" dist/images",
    "build:deploy:manifest": "cpx \"manifest.json\" dist",
    "build:deploy:sw": "cpx \"service-worker.js\" dist",
    "test": "au test"
  },
